\n    }\r\n};\r\n</script>\r\n\r\n<template>\r\n <h5>Advanced</h5>\r\n        <FileUpload name=\"demo[]\" url=\"./upload.php\" @upload=\"onUpload\" :multiple=\"true\" accept=\"image/*\" :maxFileSize=\"1000000\">\r\n            <template #empty>\r\n                <p>Drag and drop files to here to upload.</p>\r\n            </template>\r\n        </FileUpload>\r\n  <main>\r\n    <div v-if=\"!listaCSV\" id=\"painelRedefinir\">\r\n      <!-- <h3 style=\"font-size: 30px\">CSV não existente, informe </h3> -->\r\n      <div style=\"display: flex; justify-content: center; margin: 30px\">\r\n        <FileUpload mode=\"basic\" name=\"demo[]\" url=\"./upload\" />\r\n      </div>\r\n    </div>\r\n  </main>\r\n</template>\r\n\r\n<style scoped>\r\nmain {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  padding: 20px;\r\n}\r\n\r\n#painelRedefinir {\r\n  width: 100%;\r\n  min-width: 20%;\r\n  font-family: \"Avenir\", Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  color: white;\r\n  margin-top: 60px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: rgb(48, 46, 54);\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}